{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Cloudflare Workers",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "module": "esnext",
    "target": "esnext",
    "lib": ["webworker", "esnext"],
    "alwaysStrict": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "types": [
      "node",
      "@cloudflare/workers-types"
    ]
  }
}
