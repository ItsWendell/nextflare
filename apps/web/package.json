{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "vercel-build": "next build --debug",
    "build": "pnpm run next:build && pnpm run pages:build",
    "dev": "next dev",
    "next:build": "next build",
    "dev:pages": "(cd .pages && pnpm wrangler pages dev . --port 9003)",
