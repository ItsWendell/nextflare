  "dependencies": {
    "@cfpreview/vercel-to-pages": "^0.0.1",
    "@vercel/next": "^3.1.22",
    "next": "canary",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@tailwindcss/typography": "^0.5.4",
    "@types/node": "^18.7.6",
    "@types/react": "18.0.17",
    "autoprefixer": "^10.4.8",
    "eslint": "8.22.0",
    "eslint-config-custom": "workspace:*",
    "next-transpile-modules": "9.0.0",
    "nextflare": "workspace:*",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "ts-loader": "^9.3.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.7.4",
    "vercel": "^28.1.0",
